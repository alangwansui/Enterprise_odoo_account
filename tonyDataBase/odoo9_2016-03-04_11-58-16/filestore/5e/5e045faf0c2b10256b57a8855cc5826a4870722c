    -webkit-transform: scaleX(0.5) scaleY(0.5);
  }
  30% {
    opacity: 1;
    -webkit-transform: scaleX(1) scaleY(1);
  }
  100% {
    opacity: 0;
    -webkit-transform: scaleX(1) scaleY(1);
  }
} 
