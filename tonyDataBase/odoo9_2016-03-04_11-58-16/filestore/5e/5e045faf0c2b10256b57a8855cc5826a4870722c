/************** DEFINITION of ANIMATIONS *************/
@keyframes markAnim {
  0% {
    opacity: 0;
    transform: scaleX(0.5) scaleY(0.5);
  }
  30% {
    opacity: 1;
    transform: scaleX(1) scaleY(1);
  }
  100% {
    opacity: 0;
    transform: scaleX(1) scaleY(1);
  }
} 
