  0% {
    opacity: 0;
    -moz-transform: scaleX(0.5) scaleY(0.5);
  }
  30% {
    opacity: 1;
    -moz-transform: scaleX(1) scaleY(1);
  }
  100% {
    opacity: 0;
    -moz-transform: scaleX(1) scaleY(1);
  }
} 
