dom.remove(node);}
var className2,style,style2,$prev;for(var i=0;i<nodes.length;i++){node=nodes[i];if((dom.isText(node)||dom.isBR(node))&&!dom.isVisibleText(node)){remove(node);nodes.splice(i,1);i--;continue;}
