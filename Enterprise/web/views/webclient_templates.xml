                            });
                        });
                    </script>

                    <style>
                        body { overflow: auto !important; }
                    </style>
                    <script type="text/javascript" src="/web/static/test/testing.js"></script>

                    <script type="text/javascript" src="/web/static/test/registry.js"></script>
